{
    "extends": ["prettier", "airbnb-base"],
    "parserOptions": {
      "ecmaVersion": 12
    },
    "env": {
      "commonjs": true,
      "node": true
    },
    "rules": {
      "no-restricted-globals": ["error", "event", "fdescribe"],
      "no-console": 0,
      "indent": 0,
      "linebreak-style": 0,
      "consistent-return": "off",
      "no-underscore-dangle": "off",
      "object-curly-newline": "off",
      "prettier/prettier": [
        "error",
        {
          // "trailingComma": "es5",
          "singleQuote": true,
          "printWidth": 100,
          "tabWidth": 4,
          "semi": true,
          "endOfLine": "auto",
          "global-require": "off",
          "arrow-body-style": ["error", "as-needed"]
        }
      ]
    },
    "plugins": ["prettier"]
  }
  